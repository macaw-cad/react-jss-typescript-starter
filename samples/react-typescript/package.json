  "scripts": {
    "scjss-verify-setup": "scjss-verify-setup",
    "hotel": "hotel",
    "jss": "jss",
    "plop": "plop",
    "start": "npm run start:disconnected",
    "start:disconnected": "npm run bootstrap:disconnected && env-cmd .env.disconnected npm-run-all --parallel start:react:disconnected start:proxy start:watch-components",
    "start:connected": "npm run scjss-verify-setup && npm run bootstrap:connected && env-cmd .env.connected npm-run-all --parallel start:react:connected start:watch-components",
    "start:react:connected": "env-cmd .env.connected react-scripts start",
    "start:react:disconnected": "env-cmd .env.disconnected react-scripts start",
    "start:proxy": "node scripts/disconnected-mode-dev-proxy.js",
    "start:watch-components": "node scripts/generate-component-factory.js --watch",
    "start:storybook": "start-storybook -p 9001 -c .storybook -s ./public",
    "serve": "npm run serve:disconnected",
    "serve:disconnected": "npm run bootstrap:disconnected && env-cmd .env.disconnected npm-run-all --serial build-server:development --parallel start:disconnected build-server:development:watch serve:nodemon",
    "serve:connected": "npm run scjss-verify-setup && npm run bootstrap:connected && env-cmd .env.connected npm-run-all --serial build-server:development --parallel start:connected build-server:development:watch serve:nodemon",
    "serve:nodemon": "npx nodemon -r source-map-support/register --config server/nodemon.json build.server/index.js",
    "serve:server": "ts-node-dev server/index.ts",
    "build": "node scripts/generate-component-factory.js && env-cmd .env.production npm-run-all --serial build:client build:serverbundle build:manifest",
    "build-server:development:watch": "npm run build-server:development && npx chokidar server/*.ts server.bundle/*.tsx src/** -c \"npm run build-server:development\"",
    "build-server:development": "cross-env NODE_ENV=development webpack --config server/server.webpack.config.js",
    "build-server:production": "cross-env NODE_ENV=production webpack --config server/server.webpack.config.js",
    "build:manifest:en": "jss manifest --includeContent --includeDictionary --language en --manifestOutputPath ./sitecore/manifest/en",
    "build:manifest:dk": "jss manifest --includeContent --includeDictionary --language dk --manifestOutputPath ./sitecore/manifest/dk",
    "build:manifest": "npm run build:manifest:en",
    "build:client": "cross-env PUBLIC_URL=$npm_package_config_sitecoreDistPath react-scripts build",
    "build:serverbundle": "cross-env NODE_ENV=production webpack --config server.bundle/server.bundle.webpack.config.js && node scripts/env-prefix-serverbundle.js",
    "build:storybook": "build-storybook -c .storybook -o build.storybook -s ./public",
    "run-build:disconnected": "cross-env NODE_ENV=production npm-run-all --parallel run-build:server:disconnected run-build:server.disconnectedproxy",
    "run-build:connected": "cross-env NODE_ENV=production npm-run-all --parallel run-build:server:connected",
    "run-build:server:disconnected": "cross-env NODE_ENV=production env-cmd .env.disconnected node build.server/index.js",
    "run-build:server.disconnectedproxy": "cross-env NODE_ENV=production env-cmd .env.disconnected node server.disconnectedproxy/disconnected-mode-proxy.js",
    "run-build:server:connected": "cross-env NODE_ENV=production env-cmd .env.connected node build.server/index.js",
    "bootstrap:connected": "node scripts/bootstrap.js",
    "bootstrap:disconnected": "node scripts/bootstrap.js --disconnected",
    "docker:build": "node scripts/docker-do.js build",
    "docker:run": "npm run docker:run:disconnected",
    "docker:run:disconnected": "node scripts/docker-do.js run --disconnected",
    "docker:run:connected": "node scripts/docker-do.js run",
    "docker:shell": "node scripts/docker-do.js shell",
    "graphql:update": "env-cmd .env.connected node ./scripts/update-graphql-fragment-data.js",
    "lint": "tslint -p ./",
    "test": "react-scripts test --env=jsdom",
    "eject-dont-do-this": "react-scripts eject",
    "purge:readme": "npx req https://purge.jsdelivr.net/gh/macaw-interactive/react-jss-typescript-starter@develop/samples/react-typescript/README.md",
    "clean": "rimraf build build.server build.storybook sitecore/manifest sitecore/package .env.connected .env.disconnected",
    "recreate:scaffolds": "npm run plop jssClassComponent ScaffoldOfJssClassComponent && npm run plop jssFunctionComponent ScaffoldOfJssFunctionComponent",
    "generate:typescript": "graphql-codegen"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "rescripts": [
    "env"
  ]
}
