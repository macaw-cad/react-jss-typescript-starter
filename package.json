{
  "name": "hello-jss-typescript",
  "description": "Application utilizing Sitecore JavaScript Services and React (create-react-app).",
  "version": "11.0.2",
  "private": true,
  "config": {
    "appName": "hello-jss-typescript",
    "rootPlaceholders": [
      "jss-main"
    ],
    "sitecoreDistPath": "/dist/hello-jss-typescript",
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "graphQLEndpointPath": "/api/hello-jss-typescript",
    "language": "en"
  },
  "engines": {
    "node": ">=8.1",
    "npm": ">=5.6.0",
    "yarn": "yarn is not supported, please use npm"
  },
  "author": {
    "name": "Sitecore Corporation",
    "url": "https://jss.sitecore.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sitecore/jss.git"
  },
  "bugs": {
    "url": "https://github.com/sitecore/jss/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@sitecore-jss/sitecore-jss-proxy": "^11.0.2",
    "@sitecore-jss/sitecore-jss-react": "^11.0.2",
    "@sitecore-jss/sitecore-jss-tracking": "^11.0.2",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "apollo-cache-inmemory": "~1.3.12",
    "apollo-client": "~2.4.8",
    "apollo-link": "~1.2.6",
    "apollo-link-batch-http": "~1.2.6",
    "apollo-link-http": "~1.5.9",
    "apollo-link-persisted-queries": "~0.2.2",
    "axios": "~0.18.0",
    "bootstrap": "~4.2.1",
    "chokidar-cli": "^1.2.1",
    "compression": "~1.7.3",
    "dotenv": "^6.2.0",
    "express": "~4.16.4",
    "graphql": "~14.0.2",
    "graphql-tag": "~2.10.0",
    "i18next": "~13.1.2",
    "i18next-fetch-backend": "~2.0.0",
    "ipaddr.js": "~1.8.1",
    "isomorphic-fetch": "~2.2.1",
    "node-cache": "^4.2.0",
    "node-fetch": "~2.3.0",
    "node-noop": "^1.0.0",
    "nodemon": "^1.18.10",
    "react": "~16.7.0",
    "react-apollo": "~2.3.3",
    "react-dom": "~16.7.0",
    "react-helmet": "~5.2.0",
    "react-i18next": "~9.0.2",
    "react-router-dom": "~4.3.1",
    "react-scripts": "2.1.3",
    "serialize-javascript": "~1.6.1",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.3.1"
  },
  "devDependencies": {
    "@babel/register": "~7.0.0",
    "@sitecore-jss/sitecore-jss-cli": "^11.0.2",
    "@sitecore-jss/sitecore-jss-dev-tools": "^11.0.2",
    "@sitecore-jss/sitecore-jss-manifest": "^11.0.2",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.0.4",
    "@types/node": "^10.12.24",
    "babel-loader": "8.0.4",
    "babel-preset-react-app": "~7.0.0",
    "chalk": "~2.4.2",
    "cross-env": "~5.2.0",
    "graphql.macro": "~1.0.2",
    "html-loader": "~0.5.5",
    "http-proxy-middleware": "~0.19.1",
    "npm-run-all": "~4.1.5",
    "null-loader": "~0.1.1",
    "url-loader": "~1.1.2",
    "webpack": "4.19.1",
    "webpack-cli": "~3.2.1",
    "plop": "2.2.0",
    "write-yaml": "1.0.0",
    "terminal-kit": "1.26.11"
  },
  "scripts": {
    "jss": "jss",
    "start": "npm-run-all --serial bootstrap:disconnected --parallel start:react start:proxy start:watch-components",
    "start:connected": "scjss-verify-setup && npm-run-all --serial bootstrap:connected start:react start:watch-components",
    "serve": "npm-run-all --parallel build-server:development:watch serve:nodemon",
    "serve:nodemon": "pushd build.server && nodemon --config ../server/nodemon.json index.js",
    "build-server:development:watch": "npx chokidar server/*.ts src/** -c \"npm run build-server:development\"",
    "build-server:development": "cross-env-shell NODE_ENV=development \"webpack --config server/server.webpack.config.js\"",
    "build": "npm-run-all --serial bootstrap:connected build:client build:serverbundle build-server:production",
    "build-server:production": "cross-env-shell NODE_ENV=production \"webpack --config server/server.webpack.config.js\"",
    "scaffold": "node scripts/scaffold-component.js",
    "start:react": "react-scripts start",
    "start:proxy": "node scripts/disconnected-mode-proxy.js",
    "start:watch-components": "node scripts/generate-component-factory.js --watch",
    "build:client": "cross-env-shell PUBLIC_URL=$npm_package_config_sitecoreDistPath \"react-scripts build\"",
    "build:serverbundle": "cross-env-shell NODE_ENV=production \"webpack --config server.bundle/server.bundle.webpack.config.js\"",
    "bootstrap:connected": "node scripts/bootstrap.js",
    "bootstrap:disconnected": "node scripts/bootstrap.js --disconnected",
    "graphql:update": "node -r babel-register ./scripts/update-graphql-fragment-data.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
